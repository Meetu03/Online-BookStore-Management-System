target/
build/

# Maven wrapper
.mvn/wrapper/maven-wrapper.jar
.mvn/

# Keep nested target/build from being ignored if in src folders (optional safe rules)
!**/src/main/**/target/
!**/src/test/**/target/
!**/src/main/**/build/
!**/src/test/**/build/

